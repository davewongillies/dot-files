background        no
out_to_console   yes
out_to_x  no
update_interval  2
total_run_times  0
short_units  yes
pad_percents  	3

override_utf8_locale yes

  #{"full_text": "ಠ_ಠ"}
TEXT
,[
  {"full_text": "$downspeed▼ $upspeed▲","color":"\#9933CC"},
  {"full_text": "ⓛ ${loadavg 1}","color": "\#DC355F"},
  {"full_text": "ⓒ $cpu%","color": "\#DC322F"},
  {"full_text": "ⓜ ${memperc}%", "color":"\#B58900"},
  {"full_text": "ⓢ ${swapperc}%","color":"\#33CC99"},
  {"full_text": "${time %a %d %b} ${time %H:%M}"},
  {"full_text": "⌘"}
]
