[global_config]
  enabled_plugins = EditorPlugin, LaunchpadCodeURLHandler, OpenAnyFile, APTURLHandler, LaunchpadBugURLHandler, GithubSearchPlugin
  extra_styling = False
  focus = mouse
  title_hide_sizetext = True
[keybindings]
  help = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
  [[EditorPlugin]]
    command = gvim --remote-silent +{line} {filepath}
    groups = file line
    match = ([^ \t\n\r\f\v:]+?):([0-9]+)
[profiles]
  [[default]]
    background_color = "#002b36"
    background_darkness = 0.83
    cursor_color = "#aaaaaa"
    font = FantasqueSansMono Nerd Font 12
    foreground_color = "#839496"
    scrollback_infinite = True
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
